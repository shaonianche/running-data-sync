    "@svgr/plugin-svgo": "^8.1.0",
    "@vercel/analytics": "^0.1.11",
    "@vitejs/plugin-react": "^4.5.2",
    "gcoord": "^0.3.2",
    "geojson": "^0.5.0",
    "mapbox-gl": "^3.12.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^1.3.0",
    "react-map-gl": "^7.1.9",
