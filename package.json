  "description": "Yi hong",
  "author": "Yi Hong <zouzou0208@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yihong0618/running_page"
  },
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "data:clean": "rm -f scripts/data.db {GPX,TCX,FIT}_OUT/* activities/* src/static/activities.json",
    "data:download:garmin": "python scripts/garmin_sync.py",
    "data:analysis:github": "python scripts/gen_svg.py --from-db --type github --output assets/github.svg",
    "data:analysis:grid": "python scripts/gen_svg.py --from-db --type grid --output assets/grid.svg",
    "build": "vite build",
    "develop": "vite dev",
    "serve": "vite serve",
    "lint": "eslint --ext .ts,.tsx src --fix",
    "ci": "pnpm run lint && pnpm run build"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-language": "^1.0.1",
